/*
Test for arithmetic operations
*/

class Test1 {
    Void main(Int x, Int y, Int z) {
        Int k;

        // Test precedences        
        k = x + y + z;
        k = x * 3 / y;
        k = x * 3 + y / z;

        // Test associations
        k = x * (y - 3);
        k = x * (3 / y);
        k = (3 + 5) * (x / (y + 1));

        // Testing unary minus
        k = -2 + -3 - -5;
        k = --x + -y * (-z - -3);
        k = ------x;

        k = (-x * (3 - -2 * (5 + -1 * (--7 - -5))));
    }
}
